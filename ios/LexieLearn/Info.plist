	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.epatjas.lexieapp</string>
			</array>
		</dict>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>exp+lexie-app</string>
			</array>
		</dict>
	</array>
