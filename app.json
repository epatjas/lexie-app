{
  "expo": {
    "name": "LexieLearn",
    "slug": "lexie-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.epatjas.lexieapp",
      "buildNumber": "1",
      "icon": "./assets/icon.png",
      "icons": {
        "app": "./assets/icon.png"
      }
    },
    "android": {
      "package": "com.epatjas.lexieapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Lexie App to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Lexie App to access your photos to create study sets."
        }
      ],
      "expo-font",
      "expo-sqlite",
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "88553ff6-ef91-4843-9837-4385cbe0089a"
      }
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "hideExponentSplash": false
    }
  }
}
