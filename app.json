{
  "expo": {
    "name": "LexieLearn Dev",
    "slug": "lexie-app",
    "version": "3.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon_opaque.png",
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-font",
      "expo-sqlite",
      "expo-av"
    ],
    "extra": {
      "eas": {
        "projectId": "88553ff6-ef91-4843-9837-4385cbe0089a"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
